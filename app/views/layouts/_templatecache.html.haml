%script{id: "project_dropdown.html", type: "text/ng-template"}
  %h5.project{ tabindex: "-1", "bind-html-unsafe" => "match.label | limitTo:28 | typeaheadHighlight:query"} 
  %h5.company {{ match.model.client.company_name | limitTo:12 }}

  .divider

%script{id: "activity_dropdown.html", type: "text/ng-template"}
  %h5.activity{ tabindex: "-1", "bind-html-unsafe" => "match.label | typeaheadHighlight:query | limitTo:25" } 

  .divider
